[{
  "requirements": [
    { "bin": "chaos", "install": "gocl -i github.com/projectdiscovery/chaos-client -c 'cmd/chaos'" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "chaos -duc -silent -d ${INPUT} -key $(shuf -n 1 ~/.config/chaos/chaos-api.txt) | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow chaos --input domain.com --output domain.com.chaos",
  "description": "Scan subdomain enumeration with chaos"
}]
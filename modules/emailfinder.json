[{
  "requirements": [
    { "bin": "emailfinder", "install": "gocl -i github.com/rix4uni/emailfinder" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | emailfinder saved | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder skymem | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder google -e | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder bing -e | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder duckduckgo -e | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder yahoo -e | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | emailfinder yandex -e | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow emailfinder -i domain.com.subs -o domain.com.emailfinder",
  "description": "Email Enumeration with emailfinder"
}]
[{
  "requirements": [
    { "bin": "httpx", "install": "gocl -i github.com/projectdiscovery/httpx" },
    { "bin": "nuclei", "install": "gocl -i github.com/projectdiscovery/nuclei" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | httpx -duc -silent | unew -el -i -t -q httpx.subs",
    "cat httpx.subs | nuclei -duc -silent -nc -t ~/nucleihub-templates/nucleihub-templates/unauthenticated-werkzeug-rce.yaml | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow werkzeug --input domain.subs --output werkzeug.nuclei",
  "description": "Scan werkzeug rce with nuclei"
}]
[{
  "requirements": [
    { "bin": "hakrawler", "install": "gocl -i github.com/hakluke/hakrawler" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | hakrawler -insecure -t 8 -timeout 15 -d 7 2>/dev/null | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow hakrawler -i domain.com.httpx -o domain.com.hakrawler",
  "description": "Url Crawling with hakrawler"
}]
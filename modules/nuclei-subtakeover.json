[{
  "requirements": [
    { "bin": "nuclei", "install": "gocl -i github.com/projectdiscovery/nuclei" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | nuclei -duc -silent -nc -nh -tags takeover -severity info,low,medium,high,critical -retries 3 -rl 150 -t ~/.garudrecon/takeovers | unew -el ${OUTPUT}"
  ],
  "example": "garudrecon workflow nuclei-subtakeover -i domain.com.httpx -o domain.com.nuclei",
  "description": "Subdomain Takeover with nuclei"
}]

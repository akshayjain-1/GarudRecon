[{
  "requirements": [
    { "bin": "subfinder", "install": "gocl -i github.com/projectdiscovery/subfinder" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "subfinder -duc -silent -d ${INPUT} -all | unew -el -i -t -q ${OUTPUT}"
  ],
  "example": "garudrecon workflow subfinder -i domain.com -o domain.com.subfinder",
  "description": "Scan subdomain enumeration with subfinder"
}]
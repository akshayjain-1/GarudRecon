[{
  "requirements": [
    { "bin": "favirecon", "install": "gocl -i github.com/edoardottt/favirecon" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | favirecon -silent | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow favirecon --input domain.com.httpx --output domain.com.favirecon",
  "description": "Scan favicon with favirecon"
}]
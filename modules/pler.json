[{
  "requirements": [
    { "bin": "pler", "install": "pip install python-pler" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | pler --silent | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow pler --input domain.com.subs --output domain.com.pler",
  "description": "Filter Cloudflare IPs before running port scans with pler"
}]
[{
  "requirements": [
    { "bin": "waymore", "install": "pip3 install waymore --no-cache-dir" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "echo ${INPUT} | waymore -mode U -lr 3 -lcc 3 -f 2>/dev/null | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow waymore -i domain.com -o domain.com.waymore",
  "description": "Url Crawling with waymore"
}]
[{
  "requirements": [
    { "bin": "arinrange", "install": "pipx install --force git+https://github.com/rix4uni/arinrange.git" },
    { "bin": "mapcidr", "install": "go install -v github.com/projectdiscovery/mapcidr/cmd/mapcidr@latest" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | arinrange --silent 2>/dev/null | mapcidr -silent -aggregate | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow arinrange --input domain.com.orgs --output domain.com.arinrange",
  "description": ""
}]
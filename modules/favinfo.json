[{
  "requirements": [
    { "bin": "favinfo", "install": "gocl -i github.com/rix4uni/favinfo" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | favinfo -silent | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow favinfo --input domain.com.httpx --output domain.com.favinfo",
  "description": "Scan favicon with favinfo"
}]
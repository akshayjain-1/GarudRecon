[{
  "requirements": [
    { "bin": "csprecon", "install": "gocl -i github.com/edoardottt/csprecon" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | csprecon -silent | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow csprecon --input domain.com.subs --output domain.com.csprecon",
  "description": "Scan content security policy with csprecon"
}]
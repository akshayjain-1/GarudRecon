[{
  "requirements": [
    { "bin": "subfinder", "install": "gocl -i github.com/projectdiscovery/subfinder" },
    { "bin": "amass", "install": "go install -v github.com/owasp-amass/amass/v3/...@master" },
    { "bin": "subdog", "install": "gocl -i github.com/rix4uni/subdog" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "subfinder -duc -silent -dL ${INPUT} -all | unew -el -i -t -q ${OUTPUT}",
    "amass enum -nocolor -nolocaldb -passive -norecursive -noalts -df ${INPUT} 2>/dev/null | unew -el -i -t ${OUTPUT}",
    "cat ${INPUT} | subdog -silent -tools all | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow monitersubs -i wildcards.txt -o wildcard.monitersubs",
  "description": "Scan subdomain enumeration with different tools"
}]
[{
  "requirements": [
    { "bin": "certinfo", "install": "gocl -i github.com/rix4uni/certinfo" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | certinfo -silent -c 50 | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow certinfo --input domain.com.subs --output domain.com.certinfo",
  "description": "Scan certificate transperency with certinfo"
}]
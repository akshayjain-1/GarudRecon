[{
  "requirements": [
    { "bin": "amass", "install": "go install -v github.com/owasp-amass/amass/v3/...@master" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "amass enum -nocolor -nolocaldb -passive -norecursive -noalts -d ${INPUT} 2>/dev/null | unew -el -i -t ${OUTPUT}",
    "wc -l ${OUTPUT}"
  ],
  "example": "garudrecon workflow amass -i domain.com -o domain.com.amass",
  "description": "Scan subdomain enumeration with amass"
}]

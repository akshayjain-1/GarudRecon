[{
  "requirements": [
    { "bin": "ipfinder", "install": "gocl -i github.com/rix4uni/ipfinder" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "echo 'ssl:\"$INPUT\"' | ipfinder shodan --silent | unew -el -t ${OUTPUT}",
    "echo 'hostname:\"$INPUT\"' | ipfinder shodan --silent | unew -el -t ${OUTPUT}",
    "echo 'ssl.cert.subject.cn:\"$INPUT\"' | ipfinder shodan --silent | unew -el -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow ipfinder -i domain.com -o domain.com.ipfinder",
  "description": ""
}]
[{
  "requirements": [
    { "bin": "dnsx", "install": "gocl -i github.com/projectdiscovery/dnsx" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | dnsx -duc -silent -nc -a -aaaa -cname -ns -txt -srv -ptr -mx -soa -axfr -caa -retry 3 -t 100 -r ${RESOLVERS} | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow dnsx -i domain.com.subs -o domain.com.dnsx",
  "description": ""
}]
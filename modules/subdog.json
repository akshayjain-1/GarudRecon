[{
  "requirements": [
    { "bin": "subdog", "install": "gocl -i github.com/rix4uni/subdog" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "echo ${INPUT} | subdog -silent -tools all | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow subdog --input domain.com --output domain.com.subdog",
  "description": "Scan subdomain enumeration with subdog"
}]
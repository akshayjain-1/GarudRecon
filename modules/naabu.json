[{
  "requirements": [
    { "bin": "naabu", "install": "gocl -i github.com/projectdiscovery/naabu" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | naabu -duc -silent -c 25 -rate 1000 -timeout 1000 -retries 3 -top-ports 100 2>/dev/null | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow naabu -i domain.com.subs -o domain.com.naabu",
  "description": "Scan open ports with naabu"
}]
[{
  "requirements": [
    { "bin": "waybackurls", "install": "gocl -i github.com/tomnomnom/waybackurls" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "echo ${INPUT} | waybackurls | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow waybackurls --input domain.com.httpx --output domain.com.waybackurls",
  "description": "Url Crawling with waybackurls"
}]
[{
  "requirements": [
    { "bin": "shortscan", "install": "gocl -i github.com/bitquark/shortscan" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "shortscan ${INPUT} -F -s -p 1 | tee -a ${OUTPUT}"
  ],
  "example": "garudrecon workflow shortscan --input domain.com --output domain.com.shortscan",
  "description": "Scan for IIS with shortscan"
}]
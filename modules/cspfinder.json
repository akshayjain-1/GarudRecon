[{
  "requirements": [
    { "bin": "cspfinder", "install": "gocl -i github.com/rix4uni/cspfinder" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | cspfinder -silent | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow cspfinder -i domain.com.subs -o domain.com.cspfinder",
  "description": "Scan content security policy with cspfinder"
}]
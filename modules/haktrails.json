[{
  "requirements": [
    { "bin": "haktrails", "install": "gocl -i github.com/hakluke/haktrails" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "echo ${INPUT} | haktrails subdomains | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow haktrails --input domain.com --output domain.com.haktrails",
  "description": "Scan subdomain enumeration with haktrails"
}]
[{
  "requirements": [
    { "bin": "shosubgo", "install": "gocl -i github.com/incogbyte/shosubgo" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "shosubgo -d ${INPUT} -s $(shuf -n 1 ~/.config/shosubgo/shosubgo-api.txt) | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow shosubgo -i domain.com -o domain.com.shosubgo",
  "description": "Scan subdomain enumeration with shosubgo"
}]
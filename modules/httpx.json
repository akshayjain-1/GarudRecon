[{
  "requirements": [
    { "bin": "httpx", "install": "go install -v github.com/projectdiscovery/httpx/cmd/httpx@latest" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "cat ${INPUT} | httpx -duc -silent -nc -sc -title -td -cl -ct -t 50 | unew -el -i -t -q ${OUTPUT}"
  ],
  "example": "garudrecon workflow httpx --input domain.com.subs --output domain.com.httpx",
  "description": "Check alive subdomains with httpx"
}]

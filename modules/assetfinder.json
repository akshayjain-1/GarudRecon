[{
  "requirements": [
    { "bin": "assetfinder", "install": "gocl -i github.com/tomnomnom/assetfinder" },
    { "bin": "unew", "install": "go install github.com/rix4uni/unew@latest" }
  ],
  "command": [
    "assetfinder --subs-only ${INPUT} | unew -el -i -t ${OUTPUT}"
  ],
  "example": "garudrecon workflow assetfinder -i domain.com -o domain.com.assetfinder",
  "description": "Scan subdomain enumeration with assetfinder"
}]